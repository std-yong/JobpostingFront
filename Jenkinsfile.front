pipeline {
    agent {
        // 해당 노드의 레이블을 지정합니다.
        label 'build_server'
    }
    stages {
		stage('Docker iamge build') {
            steps {
                sh 'docker build -t front-web:1.0 .'
            }
            post {
                failure {
                    echo 'Docker build failure'
                }
                success {
                    echo 'Docker build success'
                }
            }
        }

    }
}
